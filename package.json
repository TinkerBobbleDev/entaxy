{
  "name": "entaxy",
  "description": "Your Personal Finance App",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://entaxy.io",
  "scripts": {
    "precommit": "lint-staged",
    "predeploy": "yarn build",
    "deploy:staging": "yarn build && aws s3 sync build/ s3://entaxy-staging",
    "start": "node scripts/start.js",
    "build:pre": "rm -rf node_modules/uri-js/dist/esnext",
    "build:src": "node scripts/build.js",
    "build": "yarn build:pre && yarn build:src",
    "test": "yarn lint && yarn test:ui",
    "test:ui": "node scripts/test.js --env=jsdom --verbose",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage --verbose",
    "test:fix": "yarn lint --fix && yarn test:ui",
    "test:watch": "node scripts/test.js --env=jsdom --watch --onlyChanged --coverage",
    "lint": "eslint src --ext '.js,.jsx' --config .eslintrc",
    "lint:fix": "yarn lint --fix",
    "flux": "flux"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^2.0.3",
    "@vx/axis": "^0.0.160",
    "@vx/curve": "^0.0.153",
    "@vx/event": "^0.0.153",
    "@vx/glyph": "^0.0.153",
    "@vx/gradient": "^0.0.153",
    "@vx/grid": "^0.0.160",
    "@vx/group": "^0.0.153",
    "@vx/mock-data": "^0.0.153",
    "@vx/pattern": "^0.0.153",
    "@vx/point": "^0.0.153",
    "@vx/responsive": "^0.0.158",
    "@vx/scale": "^0.0.153",
    "@vx/shape": "^0.0.160",
    "@vx/tooltip": "^0.0.160",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "blockstack": "^18.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-geo": "^1.10.0",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-svg-legend": "^2.25.6",
    "d3-transition": "^1.1.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "formik": "^0.11.11",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.3",
    "jest-environment-enzyme": "^6.0.0",
    "jest-enzyme": "^6.0.0",
    "jsdom": "^11.9.0",
    "lodash": "^4.17.5",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-confirm": "^0.1.17",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.1",
    "react-motion": "^0.5.2",
    "react-number-format": "^3.3.1",
    "react-popper": "^1.0.0-beta.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "typeface-roboto": "^0.0.54",
    "url-loader": "0.6.2",
    "uuid": "^3.2.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "react-test-renderer": "^16.3.1",
    "redux-mock-store": "^1.5.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint --fix",
      "yarn test:coverage",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/registerServiceWorker.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
